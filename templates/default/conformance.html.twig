{% extends 'default/base.html.twig' %}
{% block body_content %}
    <details open>
        <summary><i class="bi bi-card-checklist"></i> Conforms To</summary>
        <ul class="list-group">
            {% for conformance in conforms_to %}
                <li class="list-group-item d-flex justify-content-between align-items-end">
                    <div class="ms-2 me-auto">
                        <a class="text-decoration-none" href="{{ conformance}}"> {{ conformance }} </a>
                    </div>
                </li>
            {% endfor %}
        </ul>
    </details>
{% endblock %}